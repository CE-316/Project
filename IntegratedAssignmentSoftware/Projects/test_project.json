    "configuration": {
        "language": "csharp",
        "compile": "mcs -out:{{exe}} {{source}}",
        "run": "mono {{exe}}"
    },
    "submissionsDirectory": "Submissions",
    "testCases": [
        {
            "name": "1",
            "input": "1, 3, 8, 4, 6",
            "output": "1, 3, 4, 6, 8",
            "points": 1
        },
        {
            "name": "2",
            "input": "inputs/input2.txt",
            "output": "outputs/output2.txt",
            "points": 1
        },
        {
            "name": "edge",
            "input": "inputs/input_edge.txt",
            "output": "outputs/output_edge.txt",
            "points": 2
        }
    ]
}